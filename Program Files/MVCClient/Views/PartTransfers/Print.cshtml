@using MvcReportViewer;
@model MVCClient.ViewModels.Helpers.PrintViewModel

@Html.MvcReportViewerFluent("/" + @Model.ReportFolder + "/PartTransferSheet").ReportParameters(new { ServerName = @Model.ServerName, CatalogName = @Model.CatalogName,  SwitchSQLID = 1, StockTransferID = @Model.Id, PrintCopyID = 1 }).Attributes(new { Height = 1100, Width = 1150, style = "border: none" }).ControlSettings(new ControlSettings() { ShowParameterPrompts = false });
@Html.MvcReportViewerFluent("/" + @Model.ReportFolder + "/PartTransferSheet").ReportParameters(new { ServerName = @Model.ServerName, CatalogName = @Model.CatalogName,  SwitchSQLID = 1, StockTransferID = @Model.Id, PrintCopyID = 2 }).Attributes(new { Height = 1100, Width = 1150, style = "border: none" }).ControlSettings(new ControlSettings() { ShowParameterPrompts = false });
@Html.MvcReportViewerFluent("/" + @Model.ReportFolder + "/PartTransferDetailSheet").ReportParameters(new { ServerName = @Model.ServerName, CatalogName = @Model.CatalogName,  SwitchSQLID = 1, StockTransferID = @Model.Id, PrintCopyID = 0 }).Attributes(new { Height = 1100, Width = 1150, style = "border: none" }).ControlSettings(new ControlSettings() { ShowParameterPrompts = false });
