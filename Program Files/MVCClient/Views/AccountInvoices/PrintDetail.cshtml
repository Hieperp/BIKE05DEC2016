@using MvcReportViewer;
@model MVCClient.ViewModels.Helpers.PrintViewModel

@Html.MvcReportViewerFluent("/" + @Model.ReportFolder + "/AccountInvoiceDetailSheet").ReportParameters(new { ServerName = @Model.ServerName, CatalogName = @Model.CatalogName,  AccountInvoiceID = @Model.Id }).Attributes(new { Height = 1100, Width = 1150, style = "border: none" }).ControlSettings(new ControlSettings() { ShowParameterPrompts = false })