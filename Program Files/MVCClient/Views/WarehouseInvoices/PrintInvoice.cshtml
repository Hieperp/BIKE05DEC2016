@using MvcReportViewer;
@model MVCClient.ViewModels.Helpers.PrintViewModel

@if (@Model.ViewOptionID != 1976) { @Html.MvcReportViewerFluent("/" + @Model.ReportFolder + "/" + (@Model.PrintOptionID == 2 ? "WarehouseInvoiceSheetVehicles" : (@Model.PrintOptionID == 3 ? "WarehouseInvoiceSheetParts" : "WarehouseInvoiceSheet"))).ReportParameters(new { ServerName = @Model.ServerName, CatalogName = @Model.CatalogName, WarehouseInvoiceID = @Model.Id, PrintOptionID = @Model.PrintOptionID, ViewOptionID = @Model.ViewOptionID == 9 ? 9 : 0 }).Attributes(new { Height = 1100, Width = 1150, style = "border: none" }).ControlSettings(new ControlSettings() { ShowParameterPrompts = false }) }

@if (@Model.ViewOptionID != 9) { @Html.MvcReportViewerFluent("/" + @Model.ReportFolder + "/" + (@Model.PrintOptionID == 2 ? "WarehouseInvoiceSheetVehicles" : (@Model.PrintOptionID == 3 ? "WarehouseInvoiceSheetParts" : "WarehouseInvoiceSheet"))).ReportParameters(new { ServerName = @Model.ServerName, CatalogName = @Model.CatalogName, WarehouseInvoiceID = @Model.Id, PrintOptionID = @Model.PrintOptionID, ViewOptionID = @Model.ViewOptionID == 9 ? 9 : 1 }).Attributes(new { Height = 1100, Width = 1150, style = "border: none" }).ControlSettings(new ControlSettings() { ShowParameterPrompts = false }) }