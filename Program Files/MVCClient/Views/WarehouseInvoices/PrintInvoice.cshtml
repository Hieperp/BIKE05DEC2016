@using MvcReportViewer;
@model MVCClient.ViewModels.Helpers.PrintViewModel

@Html.MvcReportViewerFluent("/" + @Model.ReportFolder + "/" + (@Model.PrintOptionID == 2 ? "WarehouseInvoiceSheetVehicles" : (@Model.PrintOptionID == 3 ? "WarehouseInvoiceSheetParts" : "WarehouseInvoiceSheet"))).ReportParameters(new { ServerName = @Model.ServerName, CatalogName = @Model.CatalogName, WarehouseInvoiceID = @Model.Id, PrintOptionID = @Model.PrintOptionID }).Attributes(new { Height = 1100, Width = 1150, style = "border: none" }).ControlSettings(new ControlSettings() { ShowParameterPrompts = false })